{
    "name": "church/thechurch",
    "description": "The Church",
    "license": "AGPL-3.0",
    "type": "project",
    "autoload": {
        "psr-0": {
          "": "src/",
          "SymfonyStandard": "app/"
        },
        "classmap": [
          "vendor/prawnsalad/nexmo/src"
        ]
    },
    "require": {
        "php": ">=5.6.0",
        "symfony/framework-bundle": "2.6.*",
        "symfony/security-bundle": "2.6.*",
        "symfony/finder": "2.6.*",
        "symfony/serializer": "2.6.*",
        "symfony/validator": "2.6.*",
        "symfony/monolog-bundle": "~2.4",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "sensio/distribution-bundle": "~3.0.12",
        "incenteev/composer-parameter-handler": "~2.0",
        "ircmaxell/random-lib": "~1.0",
        "guzzlehttp/guzzle": "~5.0",
        "guzzlehttp/oauth-subscriber": "~0.1",
        "egulias/email-validator": "~1.0",
        "giggsey/libphonenumber-for-php": "~7.0",
        "prawnsalad/nexmo": "dev-master"
     },
     "require-dev": {
       "sensio/generator-bundle": "~2.3",
       "squizlabs/php_codesniffer": "2.*"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.6-dev"
        }
    }
}
